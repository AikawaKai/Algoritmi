	scanf("%d",&giorno);
	printf("L M M G V S D")
	for(i=0;i<4;i++)
	{
		for(j=start;j<end;j++)
		{
			if(j==giorno)
			{
				for(j=giorno;j<end;j++)
				{
					printf(" %d ",j);
				}
			}
			else
			{
				printf("  ");
			}
		}
		start=end;
		end=end+7;
		printf("\n");
	}

}